      <li>WSSpellWriter</li>
    </fixedBillGiverDefs>
    <verb>write</verb>
    <gerund>writing at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>

</Defs>